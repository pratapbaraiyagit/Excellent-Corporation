{
    "name": "excellent-web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.0",
        "@iconfu/svg-inject": "1.2.3",
        "@mui/icons-material": "7.1.1",
        "@mui/material": "7.1.1",
        "@reduxjs/toolkit": "1.9.5",
        "axios": "1.6.0",
        "bootstrap": "^5.2.3",
        "cra-template-rb": "1.2.6",
        "css-loader": "^6.7.3",
        "formik": "2.4.2",
        "fs": "0.0.1-security",
        "primeicons": "^6.0.1",
        "primereact": "^9.2.3",
        "react": "^18.2.0",
        "react-bootstrap": "^2.7.4",
        "react-dom": "^18.2.0",
        "react-inner-image-zoom": "4.0.1",
        "react-phone-input-2": "2.13.7",
        "react-range-slider-input": "3.0.7",
        "react-redux": "7.2.8",
        "react-router-dom": "6.8.1",
        "react-scripts": "5.0.1",
        "react-select": "5.8.0",
        "react-simple-star-rating": "5.1.7",
        "react-svg": "16.1.29",
        "react-table": "7.8.0",
        "react-toastify": "9.1.3",
        "sass": "1.58.3",
        "swiper": "10.3.1",
        "yup": "1.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "start:prod": "yarn run build && serve -s build",
        "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "yarn run eslint src",
        "lint:fix": "yarn run eslint --fix src",
        "lint:css": "stylelint src/**/*.css",
        "generate": "plop --plopfile internals/generators/plopfile.ts",
        "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
        "prettify": "prettier --write",
        "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=14.x"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-prettier": "5.0.0",
        "prettier": "3.0.3"
    }
}
